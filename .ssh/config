Host github.com
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_ed25519

Host k
    HostName kailash.luddy.indianapolis.iu.edu
    User mraadam
    IdentityFile ~/.ssh/id_ed25519
    SetEnv TERM=xterm-256color
    ForwardAgent yes
    LocalForward localhost:8888 127.0.0.1:8888
    LocalForward localhost:8188 127.0.0.1:8188
    LocalForward localhost:5151 127.0.0.1:5151

Host br
    HostName bigred200.uits.iu.edu
    User mraadam
    IdentityFile ~/.ssh/id_ed25519
    SetEnv TERM=xterm-256color
    ForwardAgent yes

Host quartz
    HostName quartz.uits.iu.edu
    User mraadam
    IdentityFile ~/.ssh/id_ed25519
